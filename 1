Speed.Name = "Speed"
Speed.Parent = main
Speed.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Speed.BorderColor3 = Color3.fromRGB(0, 0, 0)
Speed.Position = UDim2.new(0.0328638479, 0, 0.275862068, 0)
Speed.Size = UDim2.new(0, 75, 0, 24)
Speed.Font = Enum.Font.Cartoon
Speed.Text = "Speed"
Speed.TextColor3 = Color3.fromRGB(255, 255, 255)
Speed.TextSize = 14.000
Speed.MouseButton1Down:connect(function()
	-- Script made by Youtube: SoftVortex  Discord: Airsoft#0001
	-- Enjoy the script
	local gmt = getrawmetatable(game)
	setreadonly(gmt,false)

	local oldindex = gmt._index
	-- Repeats the walkspeed so the games anticheat can't reset the speed to its original speed
	gmt._index = newcclosure(function(self,k)
		if k == "WalkSpeed" then
			return 17
		end
		return oldindex(self,k)
	end)
	--> Prints meta to make sure script i running well
	print("meta")
	--> Hoopz original humanoid speed is 16 so now it does + 2.1 to your walkspeed
	while true do
		wait()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 18.1
	end
end)
