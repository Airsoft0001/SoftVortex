Jump.MouseButton1Down:connect(function()
	-- Script made by Youtube: SoftVortex  Discord: Airsoft#0001
	-- Enjoy the script
	local gmt = getrawmetatable(game)
	setreadonly(gmt,false)

	local oldindex = gmt._index
	-- Repeats the JumpPower so the games anticheat can't reset the speed to its original speed
	gmt._index = newcclosure(function(self,k)
		if k == "JumpPower" then
			return 20
		end
		return oldindex(self,k)
	end)
	--> Prints meta to make sure script i running well
	print("JmuPower")
	--> Hoopz original humanoid JumpPower is 50 so now it does + whatever value you choose to add to your JumpPower
	while true do
		wait()
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 70
	end

end)
